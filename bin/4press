#!/usr/bin/env node
// Generated by CoffeeScript 1.6.2
var args, command, command_name, commands, version;

args = process.argv.slice(2);

command_name = args.shift();

commands = require("../lib/command");

switch (command_name) {
  case "-v":
  case "--version":
  case "ver":
  case "version":
    version = require("../package.json").version;
    console.log(version);
    break;
  default:
    command = commands[command_name] || commands.help;
    command(args);
}
